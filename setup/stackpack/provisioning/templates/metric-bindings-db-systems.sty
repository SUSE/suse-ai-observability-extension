  - _type: MetricBinding
    name: DB Request Rate
    queries:
      - expression: sum(rate(db_requests_total{ db_system="${name}" }[${__rate_interval}]))
        alias: Request Rate
        primary: true
    scope:  type in ("genai.dbsystem.milvus") 
    identifier: urn:stackpack:genai-observability:shared:metric-binding:vectordb-system-request-rate
    unit: reqps
    chartType: line
    priority: high
    enabled: true
    layout:
      metricPerspective:
        tab: VectorDB
        section: Request 
      componentSummary:
        weight: 3

