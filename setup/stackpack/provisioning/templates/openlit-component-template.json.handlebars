{
  "_type": "Component",
  "name": "\{{#if element.data.name\}}\{{ element.data.name \}}\{{else\}}\{{ element.externalId \}}\{{/if\}}",
  "type" : \{{ resolveOrCreate "ComponentType" "urn:stackpack:open-telemetry:shared:component-type:service" "otel service" \}},
  "layer" : \{{ resolveOrCreate "Layer" "urn:stackpack:common:layer:services" "Services" \}},
  "domain": \{{ resolveOrCreate "Domain" "urn:stackpack:open-telemetry:shared:domain:opentelemetry" "Open Telemetry" \}},
  "labels": [
    \{{#if element.data.tags \}}
      \{{# join element.data.tags "," \}}
        {
          "_type": "Label",
          "name": "\{{key\}}"
        }
        ,{
          "_type": "Label",
          "name": "\{{key\}}:\{{this\}}"
        }
      \{{/ join \}}
    \{{/if\}}
  ]
}
